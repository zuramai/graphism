{
  "name": "graphism",
  "version": "0.1.2",
  "description": "A website to create tree graph and run algorithms on top of it",
  "private": true,
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuramai/graph-algorithms.git"
  },
  "author": "",
  "scripts": {
    "play:dev": "pnpm -C playground run dev -- --open",
    "play:build": "pnpm -C playground run build",
    "js:dev": "pnpm run -r --filter ./packages dev",
    "js:build": "pnpm run -r --filter ./packages build",
    "dev": "nr play:dev",
    "build": "nr js:build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "bumpp package.json packages/*/package.json --tag --commit --push && pnpm -r publish --access public"
  },
  "bugs": {
    "url": "https://github.com/zuramai/graph-algorithms/issues"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@antfu/eslint-config-ts": "^0.26.1",
    "@antfu/ni": "^0.17.2",
    "@vitest/coverage-c8": "^0.22.0",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "canvas": "^2.9.3",
    "eslint": "^8.22.0",
    "jsdom": "^20.0.0",
    "nanoevents": "^7.0.1",
    "pnpm": "^6.34.0",
    "sass": "^1.54.4",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "vitest": "^0.22.0"
  }
}
